
#include <iostream>
using namespace std;
void swap(int a[],int i,int j){
    int tmp = a[i];
    a[i] = a[j];
    a[j] = tmp;
}
void bubble_sort (int a[],int len){
    int max = len - 1;
    int i = 0, j = 0;
    for (i = 0; i < max; i++){
        for (j = 0; j < max - i; j++){
            if (a[j+1]<a[j]){
                swap(a,j,j+1);
            }
        }
    }
    cout<<"bubble_sort"<<endl;
    for (int k = 0; k<len;k++){
        cout<<a[k]<<" ";
    }
}

int main() {
	cout  << "test\n";
	int a[5] = {1,4,5,3,2};
	bubble_sort(a,5);
	return 0;
}
